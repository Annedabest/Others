{
  "name": "coaching-platform",
  "version": "0.1.0",
  "packageManager": "npm@10.5.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=@coaching-platform/api",
    "dev:web": "turbo run dev --filter=@coaching-platform/web",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format"
  },
  "devDependencies": {
    "turbo": "^2.1.1"
  }
}
